<!DOCTYPE html>
<html>

<head>
  <title>Travel Countries</title>
</head>

<body>
  <h1>Welcome to the Travel Countries Website</h1>
  <ul>
    <% countries.forEach(country=> { %>
      <li><a href="/countries/<%= country %>">
          <%= country %>
        </a></li>
      <% }); %>
  </ul>
</body>

</html>